<?xml version="1.0"?>
<data_managers>
    <data_manager tool_file="data_manager/get_TCGA.xml" id="elmer_tcga_fetcher" version="1.0.0">
        <data_table name="elmer_tcga_table">
            <output>
                <column name="value" />
                <column name="name" />
                <column name="path" output_ref="out_file">
                    <move type="directory" relativize_symlinks="True">
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">elemer_tcga_data/${value}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/elmer_tcga_data/${value}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>
